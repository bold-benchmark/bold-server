@base <http://paul.ti.rw.fau.de:8080/marmotta/resource/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix schema: <http://schema.org/> .

<iot-board-model> a schema:ProductModel .

<motherboard-model> a schema:ProductModel ;
	schema:isAccessoryOrSparePartFor <iot-board-model> .

<case-model> a schema:ProductModel ;
	schema:isAccessoryOrSparePartFor <iot-board-model> .
<case1> a schema:Product ;
	schema:model <case-model> ;
	schema:location <delivery> .
<case2> a schema:Product ;
	schema:model <case-model> ;
	schema:location <delivery> .

<processing-unit-model> a schema:ProductModel ;
	schema:isAccessoryOrSparePartFor <motherboard-model> .
<processing-unit1> a schema:Product ;
	schema:model <processing-unit-model> ;
	schema:location <delivery> .
<processing-unit2> a schema:Product ;
	schema:model <processing-unit-model> ;
	schema:location <delivery> .

<accelerometer-model> a schema:ProductModel ;
	schema:isAccessoryOrSparePartFor <motherboard-model> .
<accelerometer1> a schema:Product ;
	schema:model <accelerometer-model> ;
	schema:location <delivery> .

<temperature-sensor-model> a schema:ProductModel ;
	schema:isAccessoryOrSparePartFor <motherboard-model> .
<temperature-sensor1> a schema:Product ;
	schema:model <temperature-sensor-model> ;
	schema:location <delivery> .

<Workstation> rdfs:subclassOf schema:Place .

<delivery> a schema:Place .
<shipping> a schema:Place .
<screwing-station> a <Workstation> .
<soldering-station> a <Workstation> .

<AssembleAction> rdfs:subclassOf schema:ConsumeAction, schema:CreateAction .

<screwing> a schema:AssembleAction ;
	schema:actionStatus schema:PotentialActionStatus ;
	schema:location <screwing-station> ;
	schema:object <motherboard-model>, <case-model> ;
	schema:result <iot-board> .

<soldering-a> a schema:AssembleAction ;
	schema:actionStatus schema:PotentialActionStatus ;
	schema:location <soldering-station> ;
	schema:object <processing-unit-model>, <accelerometer-model> ;
	schema:result <motherboard-model> .

<soldering-b> a schema:AssembleAction ;
	schema:actionStatus schema:PotentialActionStatus ;
	schema:location <screwing-station> ;
	schema:object <processing-unit-model>, <temperature-sensor-model> ;
	schema:result <motherboard-model> .
